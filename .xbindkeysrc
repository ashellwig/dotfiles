###########################
# xbindkeys configuration #
###########################

#keystate_numlock = enable
#keystate_scrolllock = enable
#keystate_capslock = enable

#turn volume down
"amixer set Master 5%-"
    m:0x0 + c:122
    XF86AudioLowerVolume

#turn volume up
"amixer set Master 5%+"
    m:0x0 + c:123
    XF86AudioRaiseVolume

#mute volume
"amixer set Master toggle"
    m:0x0 + c:121
    XF86AudioMute

#
# End of xbindkeys configuration
